  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
